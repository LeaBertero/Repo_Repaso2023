Proceso sin_titulo
	
	Definir  contador como entero;
	
	escribir "Ingrese un numero para contador";
	leer contador;
	
	Mientras contador = 0 Hacer
		
		Repetir
			contador <- contador + 1;
			Escribir contador;
			Esperar Tecla;
		Hasta Que contador = 10; 
		
		
	FinMientras
	
	
	
FinProceso
